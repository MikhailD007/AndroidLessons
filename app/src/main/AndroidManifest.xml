<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.vimteam.weatherreport">

    <application
        android:name="org.vimteam.weatherreport.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_weather_report"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_weather_report"
        android:supportsRtl="true"
        android:theme="@style/Theme.WeatherReport">
        <activity
            android:name="org.vimteam.weatherreport.main.ui.activities.MainActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>